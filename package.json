{
    "name": "fifa-web-app-assistant",
    "version": "1.0.0",
    "description": "Fifa web app transfer assistant",
    "scripts": {
        "rebuild": "npm rebuild --runtime=electron --target=2.0.7 --disturl=https://atom.io/download/atom-shell --abi=64",
        "dev": "cross-env ELECTRON_CONNECT=true webpack --watch",
        "watch": "webpack --watch",
        "build": "cross-env NODE_ENV=production webpack -p",
        "build:dev": "webpack",
        "lint": "tslint --fix \"./src/**/*.ts{,x}\"",
        "prettier": "prettier --parser typescript --single-quote --tab-width 4 --write \"./src/**/*.ts{,x}\""
    },
    "browserslist": [
        "last 2 Chrome versions"
    ],
    "lint-staged": {
        "*.ts{,x}": [
            "tslint",
            "prettier --parser typescript --single-quote --tab-width 4 --write",
            "git add"
        ]
    },
    "dependencies": {
        "axios": "^0.18.0",
        "cheerio": "^1.0.0-rc.2",
        "csv": "^3.1.0",
        "electron": "^2.0.7",
        "electron-connect": "^0.6.3",
        "lodash": "^4.17.10",
        "node-notifier": "^5.2.1",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "robotjs": "0.5.1"
    },
    "devDependencies": {
        "@danila/tslint-config": "^1.0.1",
        "@types/cheerio": "^0.22.8",
        "@types/csv-parse": "^1.1.11",
        "@types/csv-stringify": "^1.4.2",
        "@types/es6-promise": "^3.3.0",
        "@types/lodash": "4.14.116",
        "@types/node": "^10.5.7",
        "@types/node-notifier": "0.0.28",
        "@types/react": "^16.4.8",
        "@types/react-dom": "^16.0.7",
        "cross-env": "^5.2.0",
        "electron-reload-webpack-plugin": "^2.0.4",
        "fork-ts-checker-webpack-plugin": "^0.4.6",
        "html-webpack-plugin": "^3.2.0",
        "node-loader": "^0.6.0",
        "ts-loader": "^4.4.2",
        "tslint": "^5.11.0",
        "husky": "^0.14.3",
        "prettier": "^1.14.2",
        "typescript": "^3.0.1",
        "webpack": "^4.16.5",
        "webpack-cli": "^3.1.0"
    }
}
